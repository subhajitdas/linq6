{
  "name": "linq6",
  "version": "1.1.0",
  "description": "A javascript LINQ API built using es6 features with deferred execution support.",
  "main": "dist/linq6.js",
  "scripts": {
    "init": "npm install",
    "lint": "node_modules/.bin/eslint --color --format stylish src",
    "prebuild": "node_modules/.bin/rimraf dist",
    "build": "node_modules/.bin/babel src --out-dir dist --source-maps --minified --ignore **/*.spec.js",
    "start": "node_modules/.bin/babel-node demo/demo.js",
    "test": "node_modules/.bin/_mocha --reporter spec tools/test-setup.js \"src/**/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subhajitdas/linq6.git"
  },
  "keywords": [
    "linq",
    "es6",
    "lazy",
    "deferred",
    "fluent"
  ],
  "author": "Subhajit Das <subhajit@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subhajitdas/linq6/issues"
  },
  "homepage": "https://github.com/subhajitdas/linq6#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "ink-docstrap": "^1.1.4",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "should": "^11.2.1",
    "should-sinon": "0.0.5",
    "sinon": "^2.1.0"
  }
}